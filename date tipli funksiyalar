using System;

namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
            DateTime dateTime = DateTime.Now;
            DateTime dateTime1 = new DateTime(2023, 09, 16);
            Console.WriteLine(dateTime1);
            Console.WriteLine(dateTime.AddDays(1));
            //Console.WriteLine(DateOnly.FromDateTime(dateTime));
            DateTime dateFromString = DateTime.Parse("2026/09/16");
            Console.WriteLine(dateFromString);
        }
    }
}

            
//"date" ile bagli temel funksiyalardir.
